#using AdaptationRules.*
#using AttrCondDefLibrary.*

#rule Context2EssentialIFML #with AdaptationRules

#source {
	++context:Context
}

#target {
	++essentialIFML:EssentialIFMLModel
}

#correspondence {
	++ contextToEssentialIFML :ContextToEssentialIFML{
		#src->context
		#trg->essentialIFML
	}
}

#attributeConditions {
	eq_string(context.name, essentialIFML.name)
}


